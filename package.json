{
  "name": "objection-swagger",
  "version": "3.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kibertoad/objection-swagger.git"
  },
  "keywords": [
    "objection",
    "objection.js",
    "swagger",
    "json",
    "schema",
    "json-schema"
  ],
  "author": "Igor Savin",
  "license": "MIT",
  "scripts": {
    "test": "mocha test/**/*.spec.js",
    "test-with-coverage": "nyc mocha --opts \"./mocha.opts\" test",
    "test-travis": "npm run eslint && nyc mocha --opts \"./mocha.opts\" test",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "eslint": "eslint --format codeframe \"lib/**/*.js\" \"test/**/*.js\""
  },
  "dependencies": {
    "es6-promisify": "^6.0.0",
    "js-yaml": "^3.11.0",
    "lodash": "^4.17.10",
    "validation-utils": "^2.0.1"
  },
  "peerDependencies": {
    "objection": " >= 1.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-plugin-prettier": "^2.6.0",
    "mkdirp-promise": "^5.0.1",
    "mocha": "^5.1.0",
    "nyc": "^11.7.1",
    "objection": "^1.1.7",
    "prettier": "^1.12.1",
    "sinon": "^5.0.1"
  },
  "nyc": {
    "description": "test coverage",
    "exclude": [
      "node_modules",
      "test",
      "build"
    ],
    "reporter": [
      "html"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/reports/coverage",
    "check-coverage": true,
    "lines": 65,
    "statements": 65,
    "functions": 75,
    "branches": 55
  }
}
